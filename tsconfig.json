{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": false,
		"erasableSyntaxOnly": true,
		"esModuleInterop": true,
		"exactOptionalPropertyTypes": true,
		"isolatedModules": true,
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"noEmit": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "ESNext",
		"types": [
			"@cloudflare/workers-types",
			"@minerva/gateway/worker-configuration.d.ts",
			"@minerva/auth/worker-configuration.d.ts"
		]
	}
}
