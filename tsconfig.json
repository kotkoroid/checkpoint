{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": false,
		"erasableSyntaxOnly": true,
		"esModuleInterop": true,
		"exactOptionalPropertyTypes": true,
		"isolatedModules": true,
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"noEmit": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "ESNext"
	},
	// TODO: https://github.com/cloudflare/workers-sdk/issues/9135
	"include": [
		"./gateway/worker-configuration.d.ts",
		"./services/auth/worker-configuration.d.ts"
	]
}
